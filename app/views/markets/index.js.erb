var $markets = $('<%= j render @markets %>');
var $container = $('.blog-articles-masonry-container');
$container.append($markets).masonry('appended', $markets);

<% if @markets.next_page %>
  $('.digg_pagination').replaceWith('<%= j will_paginate @markets %>');
<% else %>
  $(window).off('scroll');
  $('.digg_pagination').remove();
<% end %>
